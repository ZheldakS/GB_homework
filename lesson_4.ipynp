   "source": [
    "import matplotlib.pyplot as plt\n",
    "import numpy as np\n",
    "%matplotlib inline\n",
    "x=[1,2,3,4,5,6,7]\n",
    "y=[3.5,3.8,4.2,4.5,5,5.5,7]\n",
    "plt.plot(x,y)\n",
    "plt.show"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 11,
   "id": "ccdf117c",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "image/png": "\n",
